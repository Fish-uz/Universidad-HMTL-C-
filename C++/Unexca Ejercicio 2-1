//Que rellene un array con los 100 primeros n√∫meros enteros y los muestre en pantalla.

#include <iostream>
#include <conio.h>

using namespace std;

int main(){
    int numero[100];

    for(int i=0; i<=99; i++){
        numero[i] = i + 1;
        cout<<numero[i]<<endl;
    }

    getch();
    return 0;
}
//Resuelto